package abc;

public class ch2ps1 {
	public static void main(String[] args) {

		// 1.practise
	//	float a = 7/4f*9/2f;
	//System.out.println(a);

	// 2.adding 8 number encrypting to show grade.
	char grade = 'b';
	grade = (char)(grade+8);
	System.out.println(grade);
	grade = (char)(grade-8);
	System.out.println(grade);
	}
}
